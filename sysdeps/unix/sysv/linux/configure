/* end confdefs.h.  */
#include <linux/version.h>
#if LINUX_VERSION_CODE < $decnum
eat flaming death
#endif
_ACEOF
if (eval "$ac_cpp conftest.$ac_ext") 2>&5 |
  $EGREP "eat flaming death" >/dev/null 2>&1; then
  libc_minimum_kernel='too old!'
else
  libc_minimum_kernel=ok
fi
